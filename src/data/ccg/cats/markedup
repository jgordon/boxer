  1 ncsubj %l %f _
  2 obj2 %l %f
  3 dobj %l %f
# is this category in the question data?

(N/N)\N
  2 ((N{Y}/N{Y}<1>){_}\N{Z}<2>){_}
  1 ignore
  2 ignore
# is this category in the question data?

(S[q]/(S[pss]\NP))/NP
  2 ((S[q]{_}/(S[pss]{Y}<1>\NP{Z*}){Y}){_}/NP{Z}<2>){_}
  1 aux %f %l
  2 ncsubj %l %f _

(S[q]/PP)/NP
  2 ((S[q]{_}/PP{Y}<1>){_}/NP{Z}<2>){_}
  1 iobj %l %f
  2 ncsubj %l %f _

(S[wq]/(S[dcl]\NP))/N
  2 ((S[wq]{_}/(S[dcl]{Y}<1>\NP{Z*}){Y}){_}/N{Z}<2>){_}
  1 ignore
  2 det %f %l

NP/PP
  1 (NP{_}/PP{Y}<1>){_}
  1 iobj %l %f

((S[q]/NP)/NP)/NP
  3 (((S[q]{_}/NP{Y}<1>){_}/NP{Z}<2>){_}/NP{W}<3>){_}
  1 ignore
  2 ignore
  3 ignore
