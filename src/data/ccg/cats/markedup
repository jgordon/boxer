  2 ((S[b]{_}\NP{Y}<1>){_}/S[qem]{Z}<2>){_}
  1 ncsubj %l %f _
  2 ccomp _ %l %f # not really consistent with Depbank, but this one difficult to get consistent with B+C

((S\NP)\(S\NP))\NP
  2 (((S[X]{Y}\NP{Z}){Y}\(S[X]{Y}<1>\NP{Z}){Y}){_}\NP{W}<2>){_}
  1 ncmod _ %f %2 # see "clobbered two years ago"
  2 ncmod _ %f %l

S\S
  1 (S[X]{Y}\S[X]{Y}<1>){_}
  1 ncmod _ %f %l

((S[ng]\NP)/(S[to]\NP))/NP
