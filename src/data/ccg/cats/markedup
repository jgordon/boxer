  2 ncmod _ %f %l
  3 ccomp _ %l %f

((S[qem]/(S[dcl]\NP))/N)/(S[adj]\NP)
  3 (((S[qem]{_}/(S[dcl]{Y}<1>\NP{Z}){Y}){_}/N{Z}<2>){_}/(S[adj]{W}<3>\NP{V}){W}){_}
  1 ncmod _ %f %l # consistent with ((S[qem]/(S[dcl]/NP))/N)/(S[adj]\NP)
  2 ncmod _ %3 %f
  3 dobj %l %f

((S\NP)\(S\NP))/S
  2 (((S[X]{Y}\NP{Z}){Y}\(S[X]{Y}<1>\NP{Z}){Y}){_}/S{W}<2>){_}
  1 ncmod _ %f %l
  2 ncmod _ %f %l

