  2 xcomp _ %l %f =be
  2 dobj %l %f

((S\NP)\(S\NP))/(S[pss]\NP)
  2 (((S[X]{Y}\NP{Z}){Y}\(S[X]{Y}<1>\NP{Z}){Y}){_}/(S[pss]{W}<2>\NP{Z*}){W}){_}
  1 xmod _ %f %l
  2 xcomp _ %l %f

