((S[wq]/(S[q]/PP))/N)/(NP/N)
  3 (((S[wq]{_}/(S[q]{Y}<1>/PP{Z}){Y}){_}/N{W}<2>){_}/(NP{V}/N{V}){U}<3>){_}
  1 ncmod _ %f %l
  2 dobj %l %f
  3 ncmod _ %2 %f
# based on "How many abbots can the abbey afford ?" from the RASP grammar development files

S[wq]\S[wq]
  1 (S[wq]{Y}\S[wq]{Y}<1>){_}
  1 ncmod _ %f %l

(S[wq]\S[wq])/(S[dcl]\NP)
  2 ((S[wq]{Y}\S[wq]{Y}<1>){_}/(S[dcl]{Z}<2>\NP{W}){Z}){_}
  1 ncmod _ %f %l
  2 xcomp _ %l %f

S[wq]/(S[q]/PP)
  1 (S[wq]{_}/(S[q]{Y}<1>/PP{Z}){Y}){_}
  1 ignore

(S[wq]/(S[q]/PP))/(S[wq]/(S[q]/(S[adj]\NP)))
  2 ((S[wq]{_}/(S[q]{Y}/PP{Z}){Y}<1>){_}/(S[wq]{W}/(S[q]{V}/(S[adj]{U}\NP{T}){U}){V}){W}<2>){_}
  1 ncmod _ %f %l
  2 ncmod _ %f %l
# not sure about this one

((S[q]/PP)/(S[adj]\NP))/NP
  3 (((S[q]{_}/PP{Y}<1>){_}/(S[adj]{Z}<2>\NP{W}){Z}){_}/NP{V}<3>){_}
  1 iobj %l %f
  2 xcomp _ %l %f
  3 ncsubj _ %l %f

(S[wq]/(S[q]/(S[adj]\NP)))/(S[adj]\NP)
  2 ((S[wq]{_}/(S[q]{Y}<1>/(S[adj]{Z}\NP{W}){Z}){V}){_}/(S[adj]{Z}<2>\NP{W}){Z}){_}
  1 ncmod _ %f %l
  2 dobj %l %f

S[wq]\(S[dcl]/NP)
  1 (S[wq]{_}\(S[dcl]{Y}<1>/NP{Z}){Y}){_}
  ! (S[wq]{_}\(S[dcl]{Y}<1>/NP{_}){Y}){_}
  1 ignore

(S[wq]\(S[dcl]/PP))/N
  2 ((S[wq]{_}\(S[dcl]{Y}<1>/PP{Z}){Y}){_}/N{W}<2>){_}
  1 ignore
  2 ignore
# is this category in the question data?

(S[wq]\S[wq])/S[dcl]
  2 ((S[wq]{Y}\S[wq]{Y}<1>){_}/S[dcl]{Z}<2>){_}
  1 ncmod _ %f %l
  2 ccomp _ %l %f

(((S[q]/PP)/PP)/(S[adj]\NP))/NP
  4 ((((S[q]{_}/PP{Y}<1>){_}/PP{Z}<2>){_}/(S[adj]{W}<3>\NP{V}){W}){_}/NP{U}<4>){_}
  1 ignore
  2 ignore
  3 ignore
  4 ignore
# is this category in the question data?

((S[pss]\NP)/NP)/PP
  3 (((S[pss]{_}\NP{Y}<1>){_}/NP{Z}<2>){_}/PP{W}<3>){_}
  1 ncsubj %l %f _
  2 dobj %l %f
  3 iobj %l %f =PP/NP

NP/N
  1 (NP{Y}/N{Y}<1>){_}
  1 dobj %l %f

(S[wq]/(S[q]/PP))/(S[wq]/(S[q]/NP))
  1 ((S[wq]{_}/(S[q]{Y}<1>/PP{Z}){Y}){_}/(S[wq]{W}/(S[q]{V}/NP{U}){V}){W}<1>){_}
  1 ncmod _ %f %l

((S[wq]/(S[q]/PP))\(S[wq]/(S[q]/PP)))/NP
  2 (((S[wq]{Y}/(S[q]{Z}/PP{W}){Y}){_}\(S[wq]{Y}<1>/(S[q]{Z}/PP{W}){Z}){Y}){_}/NP{V}<2>){_}
  1 ncmod _ %f %l
  2 dobj %l %f

((S[wq]/(S[q]/NP))/N)/(NP/N)
  3 (((S[wq]{_}/(S[q]{Y}<1>/NP{Z*}){Y}){_}/N{Z}<2>){_}/(NP{W}/N{W}){V}<3>){_}
  1 ncmod _ %f %l
  2 dobj %l %f
  3 ncmod _ %2 %f

((S[wq]/PP)/N)/(NP/N)
  3 (((S[wq]{_}/PP{Y}<1>){_}/N{Z}<2>){_}/(NP{W}/N{W}){V}<3>){_}
  1 ncmod _ %f %l
  2 dobj %l %f
  3 ncmod _ %2 %f

(S[wq]\(S[dcl]/PP))/(S[wq]\(S[dcl]/PP))
  1 ((S[wq]{Y}\(S[dcl]{Z}/PP{W}){Z}){Y}/(S[wq]{Y}<1>\(S[dcl]{Z}/PP{W}){Z}){Y}){_}
  1 ncmod _ %f %l
# is this category in the question data?

LQU
  1 LQU{_}

RQU
  1 RQU{_}

(((S[b]\NP)/NP)/PP)/NP
  4 ((((S[b]{_}\NP{Y}<1>){_}/NP{Z}<2>){_}/PP{W}<3>){_}/NP{V}<4>){_}
  1 ncsubj _ %l %f
  2 obj2 %l %f
  3 iobj %l %f
  4 dobj %l %f
# is this category in the question data?

