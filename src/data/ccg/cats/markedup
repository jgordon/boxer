  4 (((NP{Y}\NP{Y}<1>){_}/((S[to]{Z}<2>\NP{W*}){Z}/NP{Y*}<3>){Z}){_}/NP{W}<4>){_}
  1 ncmod _ %f %l
  2 ccomp _ %l %f
  3 ignore
# one too many arguments here

