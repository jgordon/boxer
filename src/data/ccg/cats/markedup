  2 ((NP{Y}/NP{Y}<1>){_}/(S[asup]{Z}<2>\NP{W}){Z}){_}
  1 ncmod _ %f %l
  2 xcomp _ %l %f

((NP\NP)/(NP\NP))/((NP\NP)/(NP\NP))
  1 (((NP{Y}\NP{Y}){Z}/(NP{Y}\NP{Y}){Z}){W}/((NP{Y}\NP{Y}){Z}/(NP{Y}\NP{Y}){Z}){W}<1>){_}
  1 ncmod _ %f %l

((N/N)\(N/N))/N
  2 (((N{Y}/N{Y}){Z}\(N{Y}/N{Y}){Z}<2>){_}/N{W}<1>){_}
  1 dobj %l %f
  2 ncmod _ %f %l

((S[dcl]\NP)/(S[pss]\NP))/NP
  3 (((S[dcl]{_}\NP{Y}<1>){_}/(S[pss]{Z}<2>\NP{W*}){Z}){_}/NP{W}<3>){_}
  1 ncsubj %l %f _
  2 xcomp _ %l %f
  3 dobj %l %f

((S[dcl]\NP[expl])/S[for])/(S[adj]\NP)
  3 (((S[dcl]{_}\NP[expl]{Y}<1>){_}/S[for]{Z}<2>){_}/(S[adj]{W}<3>\NP{V}){W}){_}
  1 ncsubj %l %f _
  2 ccomp _ %3 %f # see "It's terrific for advertisers ..."
  3 xcomp _ %l %f

