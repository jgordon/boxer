# extra categories added to the end for Matt's CCG-PropBank work
(((S[dcl]\NP)/PP)/PP)/PP
  4 ((((S[dcl]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/PP{W}<3>){_}/PP{V}<4>){_}
  1 ncmod _ %f %l
  2 iobj _ %l %f
  3 iobj _ %l %f
  4 iobj _ %l %f

PP/N[num]
  1 (PP{_}/N[num]{Y}<1>){_}
  1 dobj _ %l %f

PP/N
  1 (PP{_}/N{Y}<1>){_}
  1 dobj _ %l %f

((S[pt]\NP)/PP)/PP
  3 (((S[pt]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/PP{W}<3>){_}
  1 ncmod _ %f %l
  2 iobj _ %l %f
  3 iobj _ %l %f

((PP/PP)/(PP/PP))\(S[adj]\NP)
  2 (((PP{_}/PP{_}){_}/(PP{_}/PP{_}){_}<1>)\(S[adj]{Y}\NP{Z}){Y}<2>){_}
  1 ncmod _ %f %l
  2 ncmod _ %f %l

((S[pt]\NP)/NP)/PP
  3 (((S[pt]{_}\NP{Y}<1>){_}/NP{Z}<2>){_}/PP{W}<3>){_}
  1 ncmod _ %f %l
  2 dobj _ %l %f
  3 iobj _ %l %f

(((S[pt]\NP)/PP)/PP)/PP
  4 ((((S[pt]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/PP{W}<3>){_}/PP{V}<4>){_}
  1 ncmod _ %f %l
  2 iobj _ %l %f
  3 iobj _ %l %f
  4 iobj _ %l %f

PP/(S[dcl]\NP)
  1 (PP{_}/(S[dcl]{Y}\NP{Z}){Y}<1>){_}
  1 cmod _ %l %f

(PP\PP)/((S\NP)\(S\NP))
  1 ((PP{Y}\PP{Y}<1>){Y}/((S{Z}\NP{V}){Z}\(S{Z}\NP{V}){Z}){Z}<2>){_}
  1 ncmod _ %f %l
  2 ncmod _ %f %l

(((S[pt]\NP)/PP)/PP)/NP
  4 ((((S[pt]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/PP{W}<3>){_}/NP{V}<4>){_}
  1 ncmod _ %f %l
  2 iobj _ %l %f
  3 iobj _ %l %f
  4 dobj _ %l %f

(((S[dcl]\NP)/PP)/NP)/PP
  4 ((((S[dcl]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/NP{W}<3>){_}/PP{V}<4>){_}
  1 ncsubj %l %f _
  2 iobj %l %f =PP/NP
  2 xcomp _ %l %f =PP/(S[ng]\NP)
  2 xcomp _ %l %f =PP/(S[adj]\NP)
  3 dobj %l %f
  4 iobj %l %f =PP/NP

((S[dcl]\NP)/(S[ng]\NP))/PP
  3 (((S[dcl]{_}\NP{Y}<1>){_}/(S[ng]{Z}<2>\NP{Y*}){Z}){_}/PP{V}<3>){_}
  1 ignore =aux
  1 ncsubj %l %f _
  2 aux %f %l =aux
  2 xcomp _ %l %f
  3 iobj %l %f

(((S[b]\NP)/PP)/NP)/PP
  4 ((((S[b]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/NP{W}<3>){_}/PP{V}<4>){_}
  1 ncsubj %l %f
  2 iobj %l %f
  3 dobj %l %f
  4 iobj %l %f

(((S[dcl]\NP)/(S[to]\NP))/PP)/PP
  4 ((((S[dcl]{_}\NP{Y}<1>){_}/(S[to]{Z}<2>\NP{W}){Z}){_}/PP{V}<3>){_}/PP{U}<4>){_}
  1 ncsubj %l %f _
  2 xcomp %f %l %k =(S[to]\NP)/(S[b]\NP)
  3 iobj %l %f =PP/NP
  4 iobj %l %f =PP/NP

(((S[ng]\NP)/PP)/NP)/PP
  4 ((((S[ng]{_}\NP{Y}<1>){_}/PP{Z}<2>){_}/NP{W}<3>){_}/PP{V}<4>){_}
  1 ncsubj %l %f _
  2 iobj %l %f =PP/NP
  2 xcomp _ %l %f =PP/(S[ng]\NP)
  2 xcomp _ %l %f =PP/(S[adj]\NP)
  3 dobj %l %f
  4 iobj %l %f

((S[b]\NP)/(S[ng]\NP))/PP
  3 (((S[b]{_}\NP{Y}<1>){_}/(S[ng]{Z}<2>\NP{W*}){Z}){_}/PP{V}<3>){_}
  1 ignore =aux
  1 ncsubj %l %f _
  2 aux %f %l =aux
  2 xcomp _ %l %f
  3 iobj %l %f

NP/N[num]
  1 (NP{_}/N[num]{Y}<1>){_}
  1 ncmod num %l %f # $ is head

(((S[dcl]\NP)/(S[to]\NP))/PP)/NP
  4 ((((S[dcl]{_}\NP{Y}<1>){_}/(S[to]{Z}<2>\NP{W}){Z}){_}/PP{V}<3>){_}/NP{U}<4>){_} 
  1 ncsubj %l %f _
  2 xcomp %f %l %k =(S[to]\NP)/(S[b]\NP)
  3 iobj %l %f =PP/NP
  4 dobj %l %f
