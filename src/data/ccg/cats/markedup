  1 (S[dcl]{_}/S[inv]{Y}<1>){_}
  1 ncsubj %f %l _ # consistent with B+C's analysis

(((S[b]\NP)/S[for])/(S[adj]\NP))/NP[expl]
  4 ((((S[b]{_}\NP{Y}<1>){_}/S[for]{Z}<2>){_}/(S[adj]{W}<3>\NP{V}){W}){_}/NP[expl]{U}<4>){_}
