  3 (((NP{Y}\NP{Y}<1>){_}/(S[dcl]{Z}<2>\NP{W}){Z}){_}\(NP{V}/NP{V}){U}<3>){_}
  1 ncmod _ %f %l
  2 ignore
  3 ncmod _ %f %l
# "all of which" construction; difficult to get looking like B+C

