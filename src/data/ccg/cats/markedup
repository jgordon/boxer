  2 ((S[q]{_}/(S[pt]{Y}<1>\NP{Z*}){Y}){_}/NP{Z}<2>){_}
  ! ((S[q]{Y}/(S[pt]{Y}<1>\NP{Z*}){Y}){Y}/NP{Z}<2>){_}
  1 aux %f %l
  2 ignore

